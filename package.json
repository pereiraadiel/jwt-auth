{
  "name": "jwt-auth",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "commit": "git add . && git commit -m",
    "push": "git push -u origin main",
    "build": "NODE_ENV=production tsc --target es2017 --experimentalDecorators --emitDecoratorMetadata",
    "start": "PROD=true node dist/server.js",
    "dev": "NODE_ENV=dev ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "mv ormconfig.js .ormconfig.js && mv .ormconfig.json ormconfig.json && ts-node-dev ./node_modules/typeorm/cli.js",
    "posttypeorm": "mv .ormconfig.js ormconfig.js && mv ormconfig.json .ormconfig.json"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.2",
    "@types/yup": "^0.29.8",
    "ts-node-dev": "^1.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3",
    "yup": "^0.29.3"
  }
}
